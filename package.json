{
  "name": "SidestBot",
  "version": "1.0.1",
  "author": "Flleeppyy",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "dev:nonodemon": "NODE_ENV=development node --inspect -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "build": "pnpm clean-dist  && tsc",
    "fetchgelbooruposts": "node cli/fetchGelbooruPosts.js",
    "clean-dist": "rimraf dist",
    "start": "NODE_ENV=production node -r tsconfig-paths/register dist/index.js",
    "lint-staged": "lint-staged",
    "lint": "prettier --write \"{src,cli}/**/*.{js,ts}\" && eslint --fix \"{src,cli}/**/*.{js,ts}\""
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/ws": "^8.2.3",
    "@types/node-telegram-bot-api": "^0.53.6",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "grunt": "^1.4.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "axios": "^0.24.0",
    "booru": "^2.5.3",
    "colors": "^1.4.0",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0",
    "express": "^4.17.3",
    "form-data": "^4.0.0",
    "masto": "^4.2.6",
    "node-telegram-bot-api": "^0.56.0",
    "pino": "^7.8.0",
    "pino-devtools": "^2.3.0",
    "pino-inspector": "^0.1.0",
    "pino-pretty": "^7.5.1",
    "pino-tee": "^0.3.0",
    "prompt": "^1.2.2",
    "socks-proxy-agent": "^6.1.1",
    "tsconfig-paths": "^3.12.0",
    "twitter-lite": "^1.1.0",
    "yargs": "^17.3.1"
  }
}
