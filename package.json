{
  "name": "picture-bot",
  "version": "1.0.0",
  "author": "Flleeppyy",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -v --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "build": "yarn tsc",
    "fetchgelbooruposts": "node cli/fetchGelbooruPosts.js",
    "clean-dist": "rimraf dist",
    "start": "NODE_ENV=production node -r tsconfig-paths/register dist/index.js",
    "lint-staged": "lint-staged",
    "lint": "prettier --write \"src/**/*.{js,ts}\" && eslint --fix \"src/**/*.{js,ts}\""
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@types/node-telegram-bot-api": "^0.53.5",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "grunt": "^1.4.1",
    "lint-staged": "^12.1.3",
    "nodemon": "^2.0.15",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "colors": "^1.4.0",
    "masto": "^4.2.4",
    "node-telegram-bot-api": "^0.56.0",
    "pino": "^7.6.3",
    "pino-pretty": "^7.3.0",
    "prompt": "^1.2.1",
    "socks-proxy-agent": "^6.1.1",
    "tsconfig-paths": "^3.12.0",
    "twitter-lite": "^1.1.0",
    "yargs": "^17.3.1"
  }
}
